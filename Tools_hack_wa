    sleep 0.03
    echo -e $white" ⟨⟩ Nomor : "$okegreen"$terget  "$white"Selamat BOS    ⟨⟩"
    sleep 0.03
    echo -e $white" ⟨⟩ gagal Mendapatkan Kode Verifikasi    ⟨⟩"
    sleep 10.00
    echo -e $white" ⟨⟩ Kode Verifikasi : $okegreen"gagal "             $white"⟨⟩"
$white" ⟨⟩ Hack Cipta "$red"Ro0TP00R $white"                 ⟨⟩"
    sleep 0.03
    echo -e $yellow " ========================================"
    sleep 0.03
else
    echo "Kesalahan"
        
