#!/bin/bash

blue='\e[0;34'
cyan='\e[0;36m'
green='\e[0;34m'
okegreen='\033[92m'
lightgreen='\e[1;32m'
white='\e[1;37m'
red='\e[1;31m'
yellow='\e[1;33m'

###################################################
# CTRL C
###################################################
trap ctrl_c INT
ctrl_c() {
clear
echo -e $red"[#]> (Ctrl + C ) Detected, Trying To Exit ... "
sleep 0.5
echo ""
echo -e $green"[#]> Terima kasih sudah make tools saya ... "
sleep 0.5
echo ""
echo -e $white"[#]> Salam Ro0TP00R "
read enter
exit
}
