load(){
    echo -e "\n"
    bar=" >>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>> "
    barlength=${#bar}
    i=0
    while((i<100)); do
        n=$((i*barlength / 100))
        printf "\e[00;32m\r[%-${barlength}s]\e[00m" "${bar:0:n}"
        ((i += RANDOM%5+2))
        sleep 0.2
    done
}

#
clear
echo -e $"Memulai Proyek Hack WhatsApp..."
load
clear
echo -e $okegreen" Welcome To Hack WhatsApp?...."
echo -e $red" Masukan Nama Asli Target Anda?....?" #tulisan keluar
echo -e $okegreen " <> "
read nick #membaca yang ditulis
clear
eye
echo -e $yellow " ================================================"
sleep 0.7
echo -e "  ==          "$okegreen "Hack WhatsApp By Ro0TP00R       "$yellow "=="
